#include "main.h"
		
#include <unistd.h>
		
/**
		
 * _putchar - writes character c to standard output
		
 * @c: The output character
		
 *
		
 * Return: 1 On success.
		
 * On error, -1 is returned, and error number is set appropriately.
		
 */
		
int _putchar(char c)
		
{
		
	return (write(1, &c, 1));
		
}
